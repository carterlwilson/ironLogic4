{
  "name": "@ironlogic4/shared",
  "version": "1.0.0",
  "description": "Shared TypeScript types and utilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./types/users": {
      "types": "./dist/types/users.d.ts",
      "import": "./dist/types/users.js",
      "require": "./dist/types/users.js"
    },
    "./types/auth": {
      "types": "./dist/types/auth.d.ts",
      "import": "./dist/types/auth.js",
      "require": "./dist/types/auth.js"
    },
    "./types/api": {
      "types": "./dist/types/api.d.ts",
      "import": "./dist/types/api.js",
      "require": "./dist/types/api.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  }
}